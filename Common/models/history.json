{
  "name": "History",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "applied": {
      "type": "boolean",
      "default": false
    },
    "favorite": {
      "type": "boolean",
      "default": false
    },
    "viewed": {
      "type": "boolean",
      "default": false
    },
    "dateTime": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "hasOne",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "$everyone",
        "permission": "DENY"
      },
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "admin",
        "permission": "ALLOW"
      },
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "candidate",
        "permission": "ALLOW"
      }
  ],
  "methods": {}
}
