      "default": false
    },
    "employer": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "admin": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "approved": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "required": true,
